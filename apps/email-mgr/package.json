{
  "name": "buf-fastify",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "tsx server.ts",
    "client": "tsx client.ts",
    "generate": "buf generate buf.build/connectrpc/eliza",
    "build": "tsc --noEmit",
    "test": "tap --typecheck --allow-empty-coverage --disable-coverage",
    "ci": "npm run generate && npm run build && npm run test",

    "email-dev": "email dev -p 3001",
    "email-export": "email export --dir=emails"
  },
  "dependencies": {
    "@fastify/cors": "^9.0.1",
    "fastify": "^4.26.0",
    "tsx": "^4.7.0",
    "esbuild": "0.20.0",
    "browserslist": "^4.22.2",
    "@react-email/components": "0.0.15-canary.1",
    "react-email": "2.1.0-canary.1",
    "react": "18.2.0",
    "@aws-sdk/client-ses": "^3.511.0",
    "@react-email/render":"^0.0.12",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/tap": "^15.0.11",
    "tap": "^18.7.0",
    "ts-node": "^10.9.2",
    "@types/react": "^18.2.38"
  }
}
